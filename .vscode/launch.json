{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Uvicorn (FastAPI) DEBUG",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "env": {
                "DEBUG": "1",
                "FASTAPISTATIC": "1"
            },
            "args": [
                "main:app",
                "--host",
                "127.0.0.1",
                "--port",
                "8000"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Uvicorn (FastAPI)",
